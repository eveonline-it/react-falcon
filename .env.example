# Example Environment Configuration
# Copy this file to .env and update with your actual values

# Development server configuration
VITE_APP_HOST=localhost
VITE_APP_PORT=3000
VITE_ESLINT_NO_DEV_ERRORS=true
VITE_PUBLIC_URL='/'
VITE_SKIP_PREFLIGHT_CHECK=true

# TinyMCE API Key (get from https://www.tiny.cloud/get-tiny/)
VITE_REACT_APP_TINYMCE_APIKEY=your_tinymce_api_key_here

# Google Maps API Key (get from https://developers.google.com/maps)
VITE_REACT_APP_GOOGLE_API_KEY=your_google_api_key_here

# EVE Online SSO Configuration
# Backend handles all EVE SSO configuration and OAuth flow
VITE_EVE_BACKEND_URL=https://your-backend-url.com

# WebSocket Configuration
# WebSocket server URL for real-time communication (use WSS for HTTPS sites)
VITE_WS_URL=wss://your-backend-url.com/websocket/connect